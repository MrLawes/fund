{
  "mcpServers": {
    "calculator": {
      "command": "python",
      "args": [
        "calculatorMCPServer.py"
      ],
      "env": null
    },
    "amap-maps": {
      "command": "npx",
      "args": [
        "-y",
        "@amap/amap-maps-mcp-server"
      ],
      "env": {
        "АМАР_MAPS_API_KEY": "${AMAP_MAPS_API_KEY}"
      }
    }
  }
}